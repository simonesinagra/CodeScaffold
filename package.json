{
	"name": "code-scaffold",
	"version": "1.0.0",
	"description": "An intuitive CLI tool that swiftly constructs project foundations. Easily generate structured codebases from templates, laying a solid groundwork for efficient development. Simplify project setup and focus on building great software.",
	"main": "dist/cli.js",
	"type": "module",
	"bin": {
		"create-project": "bin/code-scaffold.js"
	},
	"scripts": {
		"build": "rollup -c rollup.config.js",
		"dev": "rollup -c rollup.config.js --watch",
		"lint": "eslint --fix ."
	},
	"keywords": [
		"cli",
		"scaffold",
		"bootstrap"
	],
	"author": "Simone Sinagra",
	"license": "ISC",
	"devDependencies": {
		"@rollup/plugin-typescript": "11.1.1",
		"@types/inquirer": "9.0.3",
		"@types/listr": "0.14.4",
		"@types/ncp": "2.0.5",
		"@types/node": "20.2.5",
		"@typescript-eslint/eslint-plugin": "5.59.8",
		"@typescript-eslint/parser": "5.59.8",
		"eslint": "8.42.0",
		"eslint-config-prettier": "8.8.0",
		"eslint-plugin-prettier": "4.2.1",
		"prettier": "2.8.8",
		"prettier-eslint": "15.0.1",
		"rollup": "3.23.0",
		"tslib": "2.5.3",
		"typescript": "5.1.3"
	},
	"dependencies": {
		"arg": "5.0.2",
		"chalk": "5.2.0",
		"execa": "7.1.1",
		"inquirer": "9.2.6",
		"listr": "0.14.3",
		"ncp": "2.0.0",
		"pkg-install": "1.0.0"
	}
}
